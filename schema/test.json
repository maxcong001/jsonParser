{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "nssai",
    "description": "Definition of type nssai",
    "type": "object",
    "properties": {
        "Nssais": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "required": [
                    "sst"
                ],
                "properties": {
                    "sst": {
                        "type": "string",
                        "minLength": 2
                    },
                    "sd": {
                        "type": "string",
                        "minLength": 2
                    }
                }
            }
        },
        "test": {
            "type": "string",
            "pattern": "[A-Fa-f0-9]{3}"
        },
        "testInt":{
            "type": "integer",
            "minimum": 0,
            "maximum": 255
        }


    }
}